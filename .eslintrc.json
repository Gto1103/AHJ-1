{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {  
	"no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement",
		"ignoreRestSiblings"
    ],
	"no-unused-vars": [
		"error", {
		  "varsIgnorePattern": "unused",
		  "vars": "all",
		  "args": "all",
		  "ignoreRestSiblings": false
		}
	],
	 "no-console": [
		"error", {
			"allow": ["log"]
		}
	]
  }
}